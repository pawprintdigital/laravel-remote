{
    "name": "pawprintdigital/laravel-remote",
    "description": "Remote SSH access for Laravel.",
    "license": "MIT",
    "homepage": "http://oss.pawprint.io",
    "support": {
        "issues": "https://github.com/pawprintdigital/laravel-remote/issues",
        "source": "https://github.com/pawprintdigital/laravel-remote"
    },
    "authors": [
        {
            "name": "Pawprint Digital Services Ltd",
            "email": "oss@pawprint.io"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.7",
        "illuminate/filesystem": ">=5.7",
        "phpseclib/phpseclib": "^2.0",
        "laravel/helpers": "^1.0.0"
    },
    "require-dev": {
        "illuminate/console": ">=5.7",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PawprintDigital\\LaravelRemote\\RemoteServiceProvider"
            ],
            "aliases": {
                "SSH": "PawprintDigital\\LaravelRemote\\RemoteFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "PawprintDigital\\LaravelRemote\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
